{
	"name": "normative",
	"version": "1.0.0",
	"description": "Desktop app for developing a legal dictionary and managing legal terminology, built with TypeScript, Electron and MongoDB.",
	"main": "build/main.js",
	"scripts": {
		"data": "tsc && node build/main.js --deleteJson --language English",
		"data:ce": "tsc && node build/main.js --convert single --language English",
		"data:cs": "tsc && node build/main.js --convert single --language Spanish",
		"data:ume": "tsc && node build/main.js --uploadMongo single --language English",
		"data:ums": "tsc && node build/main.js --uploadMongo single --language Spanish",
		"data:dje": "tsc && node build/main.js --deleteJson --language English",
		"data:djs": "tsc && node build/main.js --deleteJson --language Spanish",
		"data:dme": "tsc && node build/main.js --deleteMongo --language English",
		"data:dms": "tsc && node build/main.js --deleteMongo --language Spanish",
		"client": "tsc && electron build/client/startup.js",
		"test": "jest --verbose --maxWorkers=1 --coverage --silent"
	},
	"keywords": [
		"legal dictionary"
	],
	"author": "Iv√°n Ovejero",
	"license": "MIT",
	"dependencies": {
		"chalk": "^4.0.0",
		"cheerio": "^1.0.0-rc.3",
		"dotenv": "^8.2.0",
		"electron": "^8.2.5",
		"firebase": "^7.14.1",
		"json-stringify-pretty-compact": "^2.0.0",
		"mammoth": "^1.4.9",
		"minimist": "^1.2.5",
		"mongodb": "^3.5.6"
	},
	"devDependencies": {
		"@types/cheerio": "^0.22.17",
		"@types/faker": "^4.1.11",
		"@types/jest": "^25.2.1",
		"@types/minimist": "^1.2.0",
		"@types/mongodb": "^3.5.14",
		"@types/node": "^12.12.38",
		"faker": "^4.1.0",
		"jest": "^25.4.0",
		"ts-jest": "^25.4.0",
		"typescript": "^3.8.3"
	}
}
